{
  "name": "@bloom-housing/public",
  "version": "7.4.3",
  "author": "Sean Albert <sean.albert@exygy.com>",
  "description": "Public web app reference implementation for the Bloom affordable housing system",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect=9230' next -p ${NEXTJS_PORT:-3000}",
    "build": "next build",
    "test": "concurrently \"yarn dev\" \"cypress open\"",
    "test:headless": "concurrently \"yarn dev\" \"cypress run\"",
    "test:coverage": "yarn nyc report --reporter=text-summary --check-coverage",
    "test:unit": "jest -w 1",
    "start": "next start -p ${NEXTJS_PORT:-3000}",
    "dev:listings": "cd ../../backend/core && yarn dev",
    "dev:server-wait": "wait-on \"http-get://localhost:${PORT:-3100}/listings\"  --httpTimeout 60000 --tcpTimeout 1500 -v --interval 15000 && yarn dev",
    "dev:server-wait-cypress": "wait-on \"http-get://localhost:${PORT:-3100}/listings\" --httpTimeout 60000 --tcpTimeout 1500 -v --interval 15000 && yarn build && yarn start",
    "dev:all-cypress": "concurrently \"yarn dev:listings\" \"yarn dev:server-wait-cypress\"",
    "dev:all": "concurrently \"yarn dev:listings\" \"yarn dev:server-wait\""
  },
  "dependencies": {
    "@bloom-housing/backend-core": "^7.11.0",
    "@bloom-housing/shared-helpers": "^7.5.0",
    "@bloom-housing/shared-services": "^1.0.0",
    "@bloom-housing/ui-components": "^11.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "autoprefixer": "^10.3.4",
    "axios": "^0.21.1",
    "dayjs": "^1.10.7",
    "dotenv": "^8.2.0",
    "nanoid": "^3.1.12",
    "next": "12.3.4",
    "next-plugin-custom-babel-config": "^1.0.2",
    "node-polyglot": "^2.4.0",
    "openapi-client-axios": "^3.6.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.5",
    "swr": "^0.5.5",
    "tailwindcss": "2.2.10"
  },
  "devDependencies": {
    "@axe-core/react": "4.4.3",
    "@babel/core": "^7.11.6",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@netlify/plugin-nextjs": "4.30.4",
    "@next/bundle-analyzer": "^10.1.0",
    "@testing-library/react": "12.1.3",
    "@types/markdown-to-jsx": "^6.11.2",
    "@types/node": "^12.12.67",
    "@types/react": "^16.9.52",
    "concurrently": "^5.3.0",
    "cypress": "^9.5.1",
    "js-levenshtein": "^1.1.6",
    "next-transpile-modules": "^8.0.0",
    "nyc": "^15.1.0",
    "postcss": "^8.3.6",
    "sass": "1.52.1",
    "sass-loader": "^10.0.3",
    "ts-loader": "^6.0.3",
    "typescript": "4.6.4",
    "webpack": "^5.69.1"
  }
}
